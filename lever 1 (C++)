return 0;
}


//10869
#include <stdio.h>

int main() {
int A, B;
scanf("%d", &A);
scanf("%d", &B);

if (A >1 && B <= 10000) {
printf("%d\n%d\n%d\n%d\n%d", A + B, A - B, A*B, A / B, A%B);
}
else printf("error");

return 0;
}


//10430

​

#include <stdio.h>

​

int main() {

int A, B, C;

scanf("%d", &A);

scanf("%d", &B);

scanf("%d", &C);

​

​

if (A >=2 && B,C <= 10000) {

printf("%d\n%d\n%d\n%d", (A + B) % C, ((A%C) + (B%C)) % C, (A * B) % C, ((A%C) * (B%C)) % C);

}

else printf("error");

​

return 0;

}


//2588
#include <stdio.h>

int main() {
int A, B;
scanf("%d", &A);
scanf("%d", &B);

int a = A*(B % 10);
int b = A*((B % 100) / 10);
int c = A*(B / 100);
printf("%d\n%d\n%d\n%d", a, b, c, a+10*b+100*c);

return 0;
}
