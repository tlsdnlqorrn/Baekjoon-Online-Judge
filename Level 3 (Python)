#11021
T=input()

for i in range(int(T)):
  A, B=input().split()
   print("Case #%d:" %(i+1), int(A)+int(B))


#11022
T=input()

for i in range(int(T)):
  A, B=input().split()
  print("Case #%d: %c + %c = %d" %(i+1, A, B, int(A)+int(B)))


#2438
N=int(input())

for i in range(1, N+1):
  print("*"*i)


#2439
N=int(input())

for i in range(1, N+1):
  print(" "*(N-i)+"*"*i)


#10871
N, X=input().split()
num=list(input().split())

for i in range(int(N)): 
  if int(num[i])<int(X):
    print(num[i])
