#2739
N=input()

for i in range(1,10):
  print(N, "*", i, "=", int(N)*i)


#10950
T=input()

for i in range(int(T)):
  A, B=input().split()
  print(int(A)+int(B))


#8393
n=input()
sum=0

if 1<=int(n) and int(n)<=10000:
  for i in range(1, int(n)+1):
    sum+=i
print(sum)

else:
  print("error")


