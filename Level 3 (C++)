//2793
#include<stdio.h>

int main(void) {
	int N;
	scanf("%d", &N);

	for (int i = 1;i < 10;i++) {
		printf("%d * %d = %d\n", N, i, N*i);
	}

	return 0;
}


//10950
#include<stdio.h>

int main(void) {
	int T = 0;
	int A, B;
	scanf("%d", &T);

	for (int i = 0;i<T;i++) {
		scanf("%d %d", &A, &B);
		printf("%d\n", A + B);
	}

	return 0;
}


//8393
#include<stdio.h>

int main(void) {
	int n;
	int sum = 0;
	scanf("%d", &n);

	if (1 <= n&&n <= 10000) {
		for (int i = 1;i < n + 1;i++) {
			sum += i;
		}
		printf("%d", sum);
	}
	else
		printf("error");

	return 0;
}


